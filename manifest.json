{
    "manifest_version": 3,
    "name": "Toufik Extension",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "browser_style": true,
        "default_icon": {
            "128": "logo.png"
        },
        "default_title": "Toufik Extension",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "logo.png"
    }
}